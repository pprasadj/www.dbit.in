extends  ../_skeleton

include ../elements/_ui-breadcrubms

block variables
  - var activeIndex = 7
  - var title = 'Page 404'

block title
  title= title

block page
  section.section-50.section-sm-75.section-md-100
      .shell.text-center
        a(href='index.html')
          img(src="images/brand-default-178x58.png", width="178", height="58", alt="ALMA")
        .range.section-75.section-sm-100.section-md-120.bg-custom.text-sm-left.offset-top-0
          .cell-sm-5.bg-custom-image.bg-custom-image-1
            img.img-responsive(src="images/page-404.jpg", width="828", height="593", alt="")
          .cell-sm-6.cell-sm-preffix-1.section-50.offset-top-0
            h1: span.biggest 404
            h2.offset-top-0  Sorry, but the page was not found
            p.offset-top-10 You may have mistyped the address or the page may have moved.
            .group.offset-top-40
              a.btn.btn-default.btn-form(href='index.html') Back to home page
              a.btn.btn-default-outline(href='contacts.html') Contact us
        p.small-xs Copyright  2018 All rights reserved by Alma University


